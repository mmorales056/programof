<h2>Listado de estudiantes</h2>
<% @students %>
<table border="1px">
  <thead>
    <tr>
      <th> Nombre</th>
      <th> Correo</th>
      <th> Fecha de Nacimiento</th>
      <th> Programa</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr>
      <td> <%= student.nombre %></td>
      <td> <%= student.correo %></td>
      <td> <%= student.fecha %></td>
      <td><%= student.programa %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<a href="/students/new"> Nuevo Estudiante</a>
